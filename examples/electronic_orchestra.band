// üåå INTRO ESPACIAL (16 compassos)
loop 8 {
    // Pads atmosf√©ricos crescendo
    play pad_strings: chord ["C1", "E1", "G1"], 30, whole;
    play pad_choir: chord ["C2", "E2", "G2"], 25, whole;
    wait quarter;
    
    play pad_strings: chord ["F1", "A1", "C2"], 35, whole;
    play pad_choir: chord ["F2", "A2", "C3"], 30, whole;
    play arp_crystal: note "C4", 40, quarter;
    wait quarter;
    play arp_crystal: note "E4", 40, quarter;
    wait quarter;
    play arp_crystal: note "G4", 40, quarter;
    wait quarter;
    play arp_crystal: note "C5", 40, quarter;
    wait quarter;
}

// ü•Å ENTRADA R√çTMICA (12 compassos)
loop 12 {
    // Bateria eletr√¥nica + bass 808
    play drums: kick, 110, quarter;
    play drums: hihat, 60, quarter;
    play bass_808: note "C1", 100, quarter;
    play pad_strings: chord ["C2", "E2", "G2"], 50, quarter;
    play arp_crystal: note "C4", 70, eighth;
    wait eighth;
    play arp_crystal: note "G4", 70, eighth;
    wait eighth;
    
    play drums: snare, 120, quarter;
    play drums: hihat, 80, quarter;
    play bass_808: note "E1", 90, quarter;
    play pad_strings: chord ["E2", "G2", "B2"], 55, quarter;
